<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="User">
	
	<select id="login" parameterType="string" resultType="string">
		SELECT user_pw FROM "USER" WHERE USER_ID = #{user_id}
	</select>

</mapper>